<div class="row full-overflow" {{action 'scrollDown'}}>
  <div class="canvas-wrap">
    {{pixi-image-distort imgUrl="assets/code.jpg"}}
  </div>

  <div class="huge-name">
    <!-- <figure>DinosaurJS</figure>
    <figure>Conference</figure> -->
    <figure class="home-circle"></figure>
  </div>
  {{outlet}}
</div>

<div class="i-am-higher">
  <section class="section section-black " id="about">

    <h3><p>
      DinosaurJS is a non-profit, community-driven JavaScript conference happening in Denver,
      Colorado on Friday, June 24th, 2016.
    </p>
    <p>One day.</p>
    <p>One track.</p></h3>
    <p><h3>
      DinosaurJS is part of the <a href="http://jsconf.com/" target="_blank">JSConf
        Family of Events</a> and adheres to the {{link-to 'JSConf Code of Conduct' '2016.code-of-conduct'}}.
      </h3>
    </p>
  </section>

  <section class="section section-white">
    <div id="mc_embed_signup">
      <form action="//dinosaurjs.us12.list-manage.com/subscribe/post?u=50315fee4889ecfe0c9a664cd&amp;id=9261db23f6" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate="">
        <div id="mc_embed_signup_scroll">
          <h2 class="margin-bottom--80">
            Sign up for updates!
          </h2>

          <div class="third">
            <div class="mc-field-group">
              <!-- <label for="mce-EMAIL">Email Address <span class="asterisk">*</span></label> -->
              <input type="email" value="" placeholder="Email*" name="EMAIL" class="required email" id="mce-EMAIL">
            </div>
            <div class="mc-field-group">
              <!-- <label for="mce-FNAME">First Name</label> -->
              <input type="text" value="" placeholder="First Name" name="FNAME" class="" id="mce-FNAME">
            </div>
            <div class="mc-field-group">
              <!-- <label for="mce-LNAME">Last Name</label> -->
              <input type="text" value="" placeholder="Last Name" name="LNAME" class="" id="mce-LNAME">
            </div>
            <div class="email-signup-submit-button">
              <input type="submit" value="Yes!" name="subscribe" id="mc-embedded-subscribe" class="button">
            </div>
          </div>

          <div id="mce-responses" class="clear">
            <div class="response" id="mce-error-response" style="display:none"></div>
            <div class="response" id="mce-success-response" style="display:none"></div>
          </div>
          <div style="position: absolute; left: -5000px;" aria-hidden="true">
            <input type="text" name="b_50315fee4889ecfe0c9a664cd_9261db23f6" tabindex="-1" value="">
          </div>
          <!-- <div class="indicates-required clear">
            <span class="asterisk">*</span> indicates required
          </div> -->

        </div>
      </form>
    </div>
  </section>


  <section class="section section-black sponsors-section">
    <h2>Sponsors</h2>
    <p>We're looking for sponsors for our wonderful event.<br>
    <a href="http://cors-files.s3.amazonaws.com/dinoJS/DinosaurJS%20Sponsorship.pdf" target="_blank">Check out our sponsor prospectus</a>.</p>
    <br>
    {{sponsors-section}}
  </section>

</div>
